{
  "body": {
    "color": "#fff",
    "background": "#2b5177"
  },
  ".playlists-tabs-li": {
    "background": "#355d85"
  },
  ".playlists-tabs-li.active": {
    "background": "#3b78b6"
  },
  ".playlist": {
    "background-image": "repeating-linear-gradient(to bottom,#355d85,#355d85 32px,#376a9c 32px,#376a9c 64px)"
  },
  ".playlist-item.selected,.item-menu-items": {
    "background": "#569bdf"
  },
  ".window": {
    "border-color": "#174575"
  },
  "#preferences-tabs": {
    "border-color": "#6eaae6",
    "background": "#7bb4ec"
  },
  ":root .preferences-item.active": {
    "background": "#3e86cf"
  },
  "#window-title": {
    "background": "#174575"
  },
  "#window-buttons": {
    "border-color": "#6eaae6",
    "background": "#abcef1"
  },
  "#volume,#progress": {
    "background": "#355d85"
  },
  "#volume-progress,#playback-progress": {
    "background": "#5a92ca"
  },
  ".add-item.active": {
    "background": "#1e6cb9"
  },
  ".add-item span": {
    "color": "#1861aa"
  },
  "h6": {
    "color": "#0c4885"
  },
  "#console-title": {
    "background": "#abcef1",
    "border-top-color": "#6eaae6"
  },
  "#add-playlist": {
    "background": "#3e86cf",
    "border-color": "#2b5177"
  },
  "#window-content": {
    "background": "#598dc0",
    "color": "#fff"
  },
  ".select:not(.top-bar)": {
    "color": "initial"
  },
  ".select select": {
    "background": "#d3e9ff"
  },
  ".top-bar select": {
    "background": "#3e86cf"
  },
  "#connected-table tr td:first-child": {
    "color": "#1861aa"
  },
  "button,.button": {
    "background": "#d3e9ff"
  },
  "#keyboard-shortcuts button:not(#shortcut-add)": {
    "color": "#3d84cc"
  },
  "#translation-contributors": {
    "background": "#6ea8e2"
  },
  "#find:focus": {
    "color": "#355d85"
  },
  "#find:focus ~ .empty-find-icon,#find:focus ~ .find-icon": {
    "color" : "#355d85"
  }
}